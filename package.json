{
	"name": "todo",
	"version": "1.0.0",
	"homepage": "https://example.com",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all --parallel watch:* start:browsersync",
		"start:server": "node server.js",
		"start:browsersync": "browser-sync start --config browser-sync-config.js",
		"deploy": "firebase deploy",
		"clean": "rimraf build/*/*",
		"chokidar": "node chokidar.js",
		"stylelint": "stylelint --config stylelint-config.json \"src/styles/*.scss\"",
		"copy:html": "copy \"src\\index.html\"  \"build\\dev\\index.html\" /Y",
		"copy:manifest": "copy \"src\\manifest.json\"",
		"build:js": "rollup --config rollup.config.js",
		"build:html": "html-minifier --config-file html-minify-config.json src/index.html --output build/prod/index.html",
		"build:sass": "node-sass src/styles",
		"build:icons": "real-favicon generate favicon-description.json favicon-data.json",
		"build:icons:all": "npm-run-all --parallel \"build:icons -- \\\"build\\dev\\\"\" \"build:icons -- \\\"build\\prod\\\"\" --serial \"copy:manifest -- \\\"build\\dev\\\"\" \"copy:manifest -- \\\"build\\prod\\\"\"",
		"build:dev": "npm-run-all --parallel build:js stylelint \"build:sass -- --output build/dev/styles\" copy:html \"copy:manifest -- \\\"build\\dev\\manifest.json\\\"\"",
		"build:prod": "npm-run-all --parallel \"build:js -- --environment NODE_ENV:production\" stylelint \"build:sass  -- --output build/prod/styles --output-style compressed\" build:html \"copy:manifest -- \\\"build\\prod\\manifest.json\\\"\" ",
		"watch:js": "npm run chokidar \"src/scripts/**/*.js\" \"npm run build:js\"",
		"watch:sass": "npm run chokidar \"src/styles/*.scss\" \"npm run build:sass\"",
		"watch:service-worker": "npm run chokidar \"src/service_worker.js\" \"npm run build:service-worker\"",
		"watch:html": "npm run chokidar \"src/index.html\" \"npm run copy:html\"",
		"watch:icons": "npm run chokidar \"src/icon.png\" \"npm run build:icons\"",
		"watch:manifest": "npm run chokidar \"src/manifest.json\" \"npm run copy:manifest \\\"build\\dev\\manifest.json\\\"\"",
		"watch": "npm-run-all --parallel watch:*"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/preset-env": "^7.10.2",
		"@babel/preset-react": "^7.10.1",
		"browser-sync": "^2.26.7",
		"chokidar": "^3.4.0",
		"cli-real-favicon": "0.0.8",
		"eslint": "^6.8.0",
		"eslint-plugin-compat": "^3.7.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.20.0",
		"express": "^4.17.1",
		"html-minifier": "^4.0.0",
		"http-proxy": "^1.18.1",
		"lodash": "^4.17.19",
		"minimist": "^1.2.5",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^2.7.1",
		"rollup": "^1.32.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-eslint": "^7.0.0",
		"rollup-plugin-gzip": "^2.5.0",
		"rollup-plugin-terser": "^5.3.0",
		"rollup-plugin-uglify": "^6.0.4",
		"stylelint": "^13.6.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-scss": "^3.17.2",
		"terser": "^4.7.0",
		"yargs-parser": "^18.1.3"
	}
}
